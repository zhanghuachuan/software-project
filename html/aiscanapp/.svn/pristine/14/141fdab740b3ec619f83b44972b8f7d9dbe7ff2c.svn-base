				function sub(){
					$.ajax({
						url:'http://localhost:8080/AiBdServer/findinfo2.action',
						data:{
							name:"chendan",
							age:'19'
						},
						type:"post",
						cache:false,
						dataType:"json",
						success:function(result){
							mui.alert("请求成功！");
							console.log(result);
						},
						error:function(e){
							mui.alert("请求失败！");
						}
					});
					
				}
